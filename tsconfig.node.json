{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "allowJs": true,
    "types": ["node", "vite/client"]
  },
  "exclude": [
    "vite.config.js",
    "dist/**/*",
    "public/**/*.js",
    "src/**/*.js",
    "*.js",
    "functions/**/*.js",
    "dataconnect-generated/**/*.js"
  ]
}