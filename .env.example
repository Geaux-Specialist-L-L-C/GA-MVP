# Environment variables for local development

# Firebase configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your_project.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Cheshire Cat configuration
VITE_CHESHIRE_API_URL=https://cheshire.geaux.app
VITE_CHESHIRE_USERNAME=your_cheshire_username_here
VITE_CHESHIRE_PASSWORD=your_cheshire_password_here
VITE_CHESHIRE_DEBUG=false

# OpenAI configuration
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_OPENAI_MODEL=gpt-4

# Azure configuration
VITE_AZURE_ENDPOINT=your_azure_endpoint_here

# Authentication settings
VITE_MAX_AUTH_RETRIES=3
VITE_USE_SECURE_COOKIES=true
VITE_AUTH_PERSISTENCE=LOCAL
VITE_AUTH_POPUP_FALLBACK=false
VITE_SERVICE_WORKER_TIMEOUT=30000
